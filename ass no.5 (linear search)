#include<stdio.h>
int main()
{
  int n,i,key,found=0;
  printf("Enter size of an array:");
  scanf("%d",&n);
  int a[n];
  printf("Array elements are:");
  for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);
   }
   
   printf("Enter a key you want to search:");
   scanf("%d",&key);
   
   for(i=0;i<n;i++)
   {
     if(a[i]==key)
     {
       printf("Element is found at position %d",i+1);
       found=1;
       break;
     }
   }
   
   if(!found)
   { 
     printf("Element not found");
   }
    
 return 0;
 
 }
