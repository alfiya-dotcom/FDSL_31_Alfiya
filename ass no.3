#include <stdio.h>
int main()
{
  int i,j,temp,n;
  
  printf("Enter size of an array:");
  scanf("%d",&n);
  
  int a[n];
  
  printf("Array elements are:");
  for(i=0;i<n;i++)
  {
     scanf("%d",&a[i]);
  }
 
  for(i=0;i<n-1;i++)
  { 
    for(j=i+1;j<n;j++)
    {
      if(a[j]<a[i])
        {
          temp=a[j];
          a[j]=a[i];
          a[i]=temp;
        }
    }
  }
  
  printf("Sorted array elements are:");
  for(i=0;i<n;i++)
  {
     printf(" %d",a[i]);
  }
  
  return 0;
}
